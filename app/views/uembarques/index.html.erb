<p id="notice"><%= notice %></p>

<h1>Uembarques</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uembarques.each do |uembarque| %>
      <tr>
        <td><%= uembarque.codigo %></td>
        <td><%= uembarque.descripcion %></td>
        <td><%= link_to 'Show', uembarque %></td>
        <td><%= link_to 'Edit', edit_uembarque_path(uembarque) %></td>
        <td><%= link_to 'Destroy', uembarque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uembarque', new_uembarque_path %>
