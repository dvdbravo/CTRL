<p id="notice"><%= notice %></p>

<h1>Parametros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Calleynumero</th>
      <th>Ciudadedocp</th>
      <th>Telefono</th>
      <th>Correoe</th>
      <th>Noentrada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parametros.each do |parametro| %>
      <tr>
        <td><%= parametro.nombre %></td>
        <td><%= parametro.calleynumero %></td>
        <td><%= parametro.ciudadedocp %></td>
        <td><%= parametro.telefono %></td>
        <td><%= parametro.correoe %></td>
        <td><%= parametro.noentrada %></td>
        <td><%= link_to 'Show', parametro %></td>
        <td><%= link_to 'Edit', edit_parametro_path(parametro) %></td>
        <td><%= link_to 'Destroy', parametro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parametro', new_parametro_path %>
